		<TitleFormat>Away is Active</TitleFormat>
		<DisplayFormat>The LED will be set to the second state if Away has been turned on, and to the first state otherwise.</DisplayFormat>
		<QueryDisplayFormat>The LED will be set to the second state if Away has been turned on, and to the first state otherwise.</QueryDisplayFormat>
	</Localization>
	<QueryClass>MiscSecurity</QueryClass>
</FunctionInfo>
*/
query bool:AwayActive()
{
	return AwayMode || Task_IsRunning(GetCurrentTask());
}

