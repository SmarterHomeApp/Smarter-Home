#if !defined(Delay)
  #define Delay Fixed
#endif

static bool:AwayMode = false;

/*
<FunctionInfo>
	<Localization Language="en">
		<DisplayName>Away</DisplayName>
		<Description>Turns loads to the specified levels after a delay the first time the button is pressed.  The second time the button is pressed, it returns loads to their previous values.</Description>
		<TitleFormat>[loads] Away</TitleFormat>
		<DisplayFormat>Turn [loads] to new levels after [OffDelay] seconds on the first press. Turn them to their previous levels on the second press.</DisplayFormat>
	</Localization>
	<BrowserClass>MiscSecurity</BrowserClass>
	<Supports>
		<QueryClass>Standard</QueryClass>
		<QueryClass>Level</QueryClass>
		<QueryClass>MiscSecurity</QueryClass>
	</Supports>
	<Join>
		<Param>loads</Param>
		<Param>awayLevels</Param>
		<Param>rampTimes</Param>
	</Join>
	<RequiredMethods>
		<MethodName>Load_Ramp</MethodName>
	</RequiredMethods>
	<Param Name="loads">
		<Localization Language="en">
			<DisplayName>Loads</DisplayName>
			<Description>The loads to be controlled.</Description>
		</Localization>
	</Param>
	<Param Name="awayLevels">
		<Localization Language="en">
			<DisplayName>Away Levels</DisplayName>
			<Description>The levels to which the loads will be set when the button is first pressed.</Description>
		</Localization>
		<SystemDefault>Level</SystemDefault>
	</Param>
	<Param Name="rampTimes">
		<Localization Language="en">
			<DisplayName>Ramp Times</DisplayName>
			<Description>The time in seconds a load takes to ramp on or fade off..</Description>
		</Localization>
		<SystemDefault>RampTime</SystemDefault>
	</Param>
	<Param Name="offDelay">
		<Localization Language="en">
			<DisplayName>Delay before Execution</DisplayName>
			<Description>The delay in seconds to wait before setting load levels the first time the button is pressed.</Description>
		</Localization>
		<DefaultValue>
			<Constant>10</Constant>
		</DefaultValue>
	</Param>
</FunctionInfo>
*/
procedure Away(Load:loads[],Level:awayLevels[], Delay:offDelay, Seconds:rampTimes[])
{
	new MemID:bufferid = GetMemID();

	if (GetEvent() != Event_ButtonUp)
	{
		new count = sizeof(loads);
		new i;

		if (AwayMode == false)
		{
			AwayMode = true;
			if (!DoesBufferExist(bufferid))
			{
				CreateBuffer(bufferid,count,true);
				for (i = 0 ; i < count ; i++)
				{
					BufferSetAt(bufferid,i,_:Load_GetLevel(loads[i]));
				}
			}
			Delay(Seconds:offDelay);

			for (i = 0 ; i < count ; i++)
			{
				Load_Ramp(loads[i],Load_RampType_Variable, rampTimes[i],awayLevels[i]);
			}
		}
		else
		{
			AwayMode = false;
			if (DoesBufferExist(bufferid))
			{
				for (i = 0 ; i < count ; i++)
				{
					Load_Ramp(loads[i],Load_RampType_Variable, rampTimes[i],Level:BufferGetAt(bufferid,i));
				}
				DestroyBuffer(bufferid);
			}
		}
	}
}

