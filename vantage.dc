		</Localization>
	</Param>
</FunctionInfo>
*/
query Number:SecurityQueries_PartitionArmState(SecurityPartition:object, SecurityPartition_ArmMode:compareValue)
{
	return Number:(SecurityPartition_ArmMode:SecurityPartition_GetArmState(object) == compareValue);
