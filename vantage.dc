/*
<FunctionInfo>
	<Localization Language="en">
		<DisplayName>Send Email</DisplayName>
		<Description>Sends an Email using the SMTP server specified in the project preferences.</Description>
		<TitleFormat>Send Email To [to] With Subject Of [subject]</TitleFormat>
		<DisplayFormat>Send Email to [to] with subject of [subject]</DisplayFormat>
	</Localization>
	<BrowserClass>Messaging</BrowserClass>
	<FunctionClasses>
		<FunctionClass>Objectless</FunctionClass>
	</FunctionClasses>
	<Param Name="to">
		<Localization Language="en">
			<DisplayName>To</DisplayName>
			<Description>A list of the recipients email addresses.  Each address should be separated by a semi-colon.  A total of 5 addresses are allowed.</Description>
		</Localization>
	</Param>
	<Param Name="from">
		<Localization Language="en">
			<DisplayName>From</DisplayName>
			<Description>The name or address of the sender.</Description>
		</Localization>
	</Param>
	<Param Name="subject">
		<Localization Language="en">
			<DisplayName>Subject</DisplayName>
			<Description>A brief description of the content of the email.</Description>
		</Localization>
	</Param>
	<Param Name="message">
		<Localization Language="en">
			<DisplayName>Message</DisplayName>
			<Description>The primary content of the email.</Description>
		</Localization>
	</Param>
	<Param Name="controller">
		<Localization Language="en">
			<DisplayName>Controller</DisplayName>
			<Description>The controller this email should be sent from.  The controller must have an open external ethernet connection.</Description>
		</Localization>
	</Param>
</FunctionInfo>
*/
procedure SendEmailWrapper(Text:to[], Text:from[], Text:subject[], Text:message[], Master:Controller)
{
	new Text:actualTo[5][50 char];
	new strLen = StrLength(to);
	new startIndex = 0;
	new toIndex = 0;

	for (new i = 0 ; i < strLen ; i++)
	{
		new temp[1 char];
		StrFormat(temp, "%c", _:to{i});
		if ( toIndex < 5 && StrCompare( temp, ";", true ) == 0 )
		{
			StrMid(actualTo[toIndex], to, startIndex, i - startIndex);
			startIndex = i + 1;
			toIndex++;
		}
	}
	if (toIndex < 5 && startIndex < strLen - 1)
	{
		StrMid(actualTo[toIndex], to, startIndex, strLen - startIndex);
	}
	
	SendEmail(actualTo, subject, message, from);
}?>
